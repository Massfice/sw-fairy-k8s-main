{
    "name": "sw-fairy-k8s-main",
    "version": "1.0.0",
    "description": "Main microservice for Shinobi War Fairy project",
    "main": "./src/server.js",
    "private": true,
    "dependencies": {
        "@types/node": "^12.0.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "html-webpack-plugin": "^5.3.2",
        "http-proxy": "^1.18.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "ts-loader": "^9.2.4",
        "typescript": "^4.1.2",
        "webpack": "^5.46.0",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.2"
    },
    "scripts": {
        "start": "webpack serve --open",
        "server:start": "node ./src/server.js",
        "serve": "dapr run --app-id main --app-port 3000 --dapr-http-port 3500 yarn start",
        "server:serve": "yarn build && dapr run --app-id main --app-port 3000 --dapr-http-port 3500 yarn server:start",
        "build": "webpack --mode=production",
        "lint": "eslint src/**/*.ts*",
        "husky:init": "husky install && husky add .husky/pre-commit 'npm run lint'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Massfice/sw-fairy-k8s-main.git"
    },
    "keywords": [
        "shinobi",
        "war",
        "fairy",
        "main"
    ],
    "author": "Massfice",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Massfice/sw-fairy-k8s-main/issues"
    },
    "homepage": "https://github.com/Massfice/sw-fairy-k8s-main#readme",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@massfice/sw-fairy-common-deploy-scripts": "^1.3.2",
        "@types/react-router": "^5.1.16",
        "@types/react-router-dom": "^5.1.8",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.28.5",
        "eslint": "^7.31.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "gulp": "^4.0.2",
        "husky": "^7.0.1",
        "prettier": "^2.3.2"
    }
}
